<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.example.applaud.filemover</string>
    <key>ProgramArguments</key>
    <array>
        <string>/ABSOLUTE/PATH/TO/YOUR/AppLaud/script/file_mover.sh</string>
        <string>/Volumes/YOUR_RECORDER_NAME</string>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>WatchPaths</key>
    <array>
        <string>/Volumes/YOUR_RECORDER_NAME</string>
    </array>
    <key>StandardOutPath</key>
    <string>/tmp/com.example.applaud.filemover.stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.example.applaud.filemover.stderr.log</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/bin:/usr/bin:/usr/local/bin:/opt/homebrew/bin:/opt/homebrew/sbin</string>
        <key>GOOGLE_API_KEY</key>
        <string>YOUR_GOOGLE_API_KEY</string>
    </dict>
    <key>WorkingDirectory</key>
    <string>/ABSOLUTE/PATH/TO/YOUR/AppLaud/script</string>
    <key>SessionCreate</key>
    <true/>
    <key>StandardInPath</key>
    <string>/dev/null</string>
</dict>
</plist> 